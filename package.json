{
  "author": "Equipo 10 Coderhouse",
  "description": "Semillero Digital es una ONG que desarrolla tus habilidades a trav√©s de sus programas de entrenamiento digital",
  "license": "MIT",
  "name": "semillero-digital",
  "scripts": {
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-css": "npm run css && npm run css-autoprefixer",
    "css": "cross-var node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js --output-style compressed src/scss/main.scss src/css/styles.css",
    "css-autoprefixer": "cross-var \"postcss -u autoprefixer -r src/css/*\""
  },
  "version": "2.0.0",
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "cross-var": "^1.1.0",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "postcss-cli": "^7.1.2"
  },
  "dependencies": {
    "nodemon": "^2.0.6"
  }
}